{
    "breadcrumbs.enabled": true,
    "C_Cpp.default.cppStandard": "c++11",
    "C_Cpp.default.cStandard": "c11",
    "C_Cpp.intelliSenseCacheSize": 10240,
    "C_Cpp.suggestSnippets": false,
    "C_Cpp.workspaceParsingPriority": "high",
    "C_Cpp.workspaceSymbols": "All",
    "C_Cpp.clang_format_path": "/usr/bin/clang-format-18",
    "debug.showInlineBreakpointCandidates": false,
    "editor.bracketPairColorization.enabled": true,
    "editor.copyWithSyntaxHighlighting": true,
    "editor.fontFamily": "'JetBrains Mono', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontLigatures": true,
    "editor.glyphMargin": false,
    "editor.minimap.maxColumn": 80,
    "editor.minimap.showSlider": "always",
    "editor.minimap.size": "fit",
    "editor.renderControlCharacters": false,
    "editor.renderWhitespace": "boundary",
    "editor.rulers": [ 80, 100, 120 ],
    "[c]": {
        "editor.formatOnSave": true
    },
    "extensions.ignoreRecommendations": false,
    "files.associations": {
        "*.*log*": "log",
        "Dockerfile*": "dockerfile",
        "Makefile*": "makefile",
    },
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true,
    },
    "files.defaultLanguage": "log",
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "git.autoRepositoryDetection": "openEditors",
    "git.detectSubmodules": false,
    "git.inputValidationSubjectLength": null,
    "git.suggestSmartCommit": false,
    "gitlens.currentLine.scrollable": false,
    "gitlens.defaultDateFormat": "DD-MM-YYYY HH:MM",
    "gitlens.defaultDateShortFormat": "DD-MM-YYYY",
    "gitlens.defaultTimeFormat": "HH:mm",
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.hovers.enabled": false,
    "gitlens.statusBar.enabled": false,
    "logFileHighlighter.customPatterns": [
        {
            "pattern": "^.*NBV.*$",
            "foreground": "#aa44ff"
        },
        {
            "pattern": "^.* ?(ERROR|FATAL) .*$",
            "foreground": "#df1f1f"
        },
        {
            "pattern": "^.* ?WARN .*$",
            "foreground": "#ffcc1f"
        },
        {
            "pattern": "^.* ?INFO .*$",
            "foreground": "#209920"
        },
        {
            "pattern": "^.* ?DEBUG .*$",
            "foreground": "#007acc"
        },
        {
            "pattern": "^.* ?TRACE .*$",
            "foreground": "#f9b9ff"
        },
    ],
    "markdown.extension.orderedList.marker": "one",
    "markdown.extension.toc.updateOnSave": false,
    "markdown.preview.doubleClickToSwitchToEditor": false,
    "plantuml.render": "PlantUMLServer",
    "scm.providerCountBadge": "auto",
    "search.showLineNumbers": true,
    "security.workspace.trust.banner": "never",
    "security.workspace.trust.enabled": false,
    "security.workspace.trust.startupPrompt": "never",
    "security.workspace.trust.untrustedFiles": "open",
    "telemetry.telemetryLevel": "error",
    "terminal.integrated.defaultProfile.linux": "zsh", // Explorer, Search, Git, Debug, Extension pane
    "workbench.enableExperiments": false,
    "workbench.colorCustomizations": {
        "editorRuler.foreground": "#353d4994",
        // Uncomment the following 2 lines to remove green higlight in diff editor
        // "diffEditor.insertedLineBackground": "#ff00ff00",
        // "diffEditor.insertedTextBackground": "#ff00ff00",
    },
    "workbench.sideBar.location": "right",
    "workbench.startupEditor": "newUntitledFile",
    "go.inlayHints.constantValues": true,
    "go.languageServerExperimentalFeatures": {
        "diagnostics": false
    },
    "go.lintOnSave": "off",
    "go.lintTool": "golangci-lint",
    "go.toolsManagement.checkForUpdates": "off",
    "go.vetOnSave": "off",
    "go.useLanguageServer": false,
    "go.formatTool": "gofmt",
    "git.mergeEditor": false,
    "gitlens.codeLens.enabled": false,
    "java.autobuild.enabled": false,
    "java.completion.guessMethodArguments": false,
    "java.progressReports.enabled": false,
    "window.zoomLevel": 0,
    "diffEditor.ignoreTrimWhitespace": false,
    "workbench.editor.highlightModifiedTabs": true,
    "[markdown]": {
        "editor.wordWrap": "on"
    },
    "plantuml.server": "http://localhost:8080",
    "sonarlint.disableTelemetry": true,
    "sonarlint.rules": {
        "c:S1659": {
            "level": "off"
        },
        "c:S1199": {
            "level": "off"
        },
        "java:S125": {
            "level": "off"
        },
        "java:S1135": {
            "level": "off"
        },
        "java:S1134": {
            "level": "off"
        },
        "python:S1134": {
            "level": "off"
        },
        "java:S112": {
            "level": "off"
        },
        "python:S125": {
            "level": "off"
        },
        "python:S1186": {
            "level": "off"
        },
        "c:S1134": {
            "level": "off"
        },
        "c:S134": {
            "level": "off"
        },
        "c:S924": {
            "level": "off"
        },
        "c:S1301": {
            "level": "off"
        },
        "c:S3776": {
            "level": "off"
        },
        "c:S1116": {
            "level": "off"
        },
        "c:S125": {
            "level": "off"
        },
        "c:S107": {
            "level": "off"
        },
        "c:S5276": {
            "level": "off"
        },
        "c:S859": {
            "level": "off"
        },
        "c:S1066": {
            "level": "off"
        }
    },
    "redhat.telemetry.enabled": true,
    "java.jdt.ls.protobufSupport.enabled": false,
    "java.compile.nullAnalysis.mode": "disabled",
    "git.openRepositoryInParentFolders": "never",
    "cSpell.userWords": [
        "ackd",
        "acked",
        "acking",
        "antireplay",
        "apdu",
        "bindgen",
        "bolosctl",
        "bolosrs",
        "DESYNC",
        "desynchronization",
        "desynchronized",
        "diversifier",
        "elfs",
        "endianness",
        "hexlify",
        "Hkdf",
        "HMAC",
        "idempotency",
        "implem",
        "logentry",
        "Merkle",
        "Misformatted",
        "Osland",
        "PCLE",
        "perso",
        "privkey",
        "prng",
        "prost",
        "proto",
        "protobuf",
        "pseudorandomly",
        "pubkey",
        "rvvm",
        "rvvmrs",
        "safenet",
        "secp",
        "Simu",
        "smfs",
        "spki",
        "syscall",
        "Uncipher",
        "uninit",
        "Unwrappable",
        "userland",
        "UTST",
        "Zeroize"
    ],
    "sonarlint.trace.server": "off",
    "rust-analyzer.cargo.extraEnv": {
        "PROJECT_DIR": "${workspaceFolder}",
        "PROJECT_BUILD_DIR": "${workspaceFolder}/_build/dev-simu"
    },
    "rust-analyzer.cargo.features": [
        "pki",
        "program",
    ],
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust-analyzer",
        "editor.formatOnSave": true
    },
    "diffEditor.maxComputationTime": 0,
    "gitlens.launchpad.indicator.enabled": false,
}
