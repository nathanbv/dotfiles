[keybindings]
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      profile = default
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
[plugins]
  [[CustomCommandsMenu]]
    [[[Android PM logs]]]
      command = logcat -v color | grep -e PM_ -e AndroidRuntime -e [Mm][Qq][Tt][Tt] -e SecurityManager -e avc
      enabled = True
      name = Android PM logs
      position = 0
    [[[Enter chroot]]]
      command = 'su root sh -c "PATH=${PATH}:/usr/bin:/usr/sbin:/bin chroot /vendor/pulsar"'
      enabled = True
      name = Enter chroot
      position = 1
